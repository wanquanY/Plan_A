# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=#填写数据库密码
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=freewrite

# API
API_V1_STR=/api/v1
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=60000000

# Logging
LOG_LEVEL=info
CONSOLE_LOG_LEVEL=info
FILE_LOG_LEVEL=info
LOG_DIR=logs
LOG_ROTATION=size
LOG_MAX_BYTES=10485760  
LOG_BACKUP_COUNT=30
SQLALCHEMY_ECHO=false

# OpenAI API
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=your_openai_api_base_url
OPENAI_MODEL=gpt-4.1-2025-04-14

# Frontend
VITE_API_BASE_URL=http://localhost:1314/api/v1 
TAVILY_API_KEY=
DEFAULT_AGENT_MODEL=gpt-4.1-2025-04-14
DEFAULT_AGENT_SYSTEM_PROMPT="你是一个强大的AI助手，你的任务是帮助用户完成各种任务。你应该：1. 始终保持友好和专业 2. 提供准确和有用的信息 3. 在需要时寻求澄清 4. 遵循用户的指示"
DEFAULT_AGENT_MAX_MEMORY=100
DEFAULT_AGENT_MODEL_SETTINGS={"temperature": 0.7, "top_p": 0.95, "presence_penalty": 0, "frequency_penalty": 0}
DEFAULT_AGENT_TOOLS_ENABLED={"note_reader": {"enabled": true, "name": "note_reader"}, "note_editor": {"enabled": true, "name": "note_editor"}, "serper_search": {"enabled": true, "name": "serper_search"}, "serper_news": {"enabled": true, "name": "serper_news"}, "serper_scrape": {"enabled": true, "name": "serper_scrape"}}
